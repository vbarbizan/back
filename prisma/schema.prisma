datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Product {
  id Int @id @default(autoincrement())
  name String
  description String
  price Float 
  stock Int
}

model Customers{
  id Int @id @default(autoincrement())
  name String
  description String
  price Float 
  stock Int
}